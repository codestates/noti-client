 history = useHistory();\n  const handleClick = () => setClick(!click);\n\n  return (\n    <ProductItemBlock>\n      <ItemImg >{img}</ItemImg>\n      <Date done={done}>{date}</Date>\n      <ProductName done={done}>{productname}</ProductName>\n      <Button done={done && <AiOutlineHeart />} onClick={onChange}>{done && <AiFillHeart />}</Button>\n      {/* <Router> */}\n      <Link\n      exact\n      to=\"/product/:productId\"\n      onClick={handleClick}>\n        상세정보 확인하기\n      </Link>\n      {/* </Router> */}\n    </ProductItemBlock>\n  );\n}\n\nexport default ProductItem;","/Users/seunghunhan/Desktop/project/noti-client/src/AlarmListPage.js",[],"/Users/seunghunhan/Desktop/project/noti-client/src/LandingPage/Toast.js",[],"/Users/seunghunhan/Desktop/project/noti-client/src/LandingPage/SearchBar.js",[],"/Users/seunghunhan/Desktop/project/noti-client/src/assets/state.js",[],"/Users/seunghunhan/Desktop/project/noti-client/src/components/LoginPage/LoginPage.js",["175"],"import React, { useState } from 'react';\nimport { useHistory, Link } from 'react-router-dom';\nimport GoogleLogin from \"react-google-login\";\nimport axios from 'axios';\nimport swal from \"sweetalert\";\n\naxios.defaults.withCredentials = true;\n \nexport default function LoginPage({ loginHandler,issueAccessToken, accessToken, token }) {\n  \n  const [details, setDetails] = useState({email: \"\", password: \"\"})\n\n  const history = useHistory();\n\n  const submitHandler = (e) =>{\n    e.preventDefault();\n \n    const { email, password } = details;\n\n    if(!email || !password){\n      return swal(\"Oops\", \"모든 항목은 필수입니다.\", \"error\");\n    }\n\n    axios.post(\"https://localhost:4000/users/login\", \n    {\n      email,\n      password\n    },\n    {\n      headers: {\n        \"Content-Type\": \"application/json\",\n      }\n    })\n    .then(res => {\n      history.push(\"/\");\n      loginHandler(res.data)\n    })\n    .catch(err => {\n      console.log(err.message);\n      alert(err.message)\n    })\n  }\n\n\n  return (\n\n    <div className=\"container\">\n      <h2>Log in</h2>\n      <form onSubmit={submitHandler}>\n        <div className=\"email-form\">\n          📧 이메일\n          <input \n            className=\"input\"\n            type=\"text\"\n            placeholder=\"이메일을 입력해주세요.\"\n            value={details.email}\n            onChange={(e) => setDetails({...details, email: e.target.value})}\n          />\n        </div>\n        <div className=\"password-form\">\n          🔐 비밀번호\n          <input \n            className=\"input\"\n            type=\"password\"\n            placeholder=\"비밀번호를 입력해주세요.\"\n            value={details.password}\n            onChange={(e) => setDetails({...details, password: e.target.value})}\n          />\n        </div>\n        <div>\n          <button className=\"btn-signin\" onClick={submitHandler}>\n          이메일로 시작하기\n          </button>\n        </div>\n       </form>   \n          {/* <div>\n          <GoogleLogin\n            className=\"btn-google\"\n            clientId=\"996092186048-291mg21lf890quda77fdgrqn11il9c0h.apps.googleusercontent.com\"\n            buttonText=\"구글 계정으로 시작하기\"\n            onSuccess={handleLoginSuccess}\n            onFailure={handleLoginFailure}\n            cookiePolicy={\"single_host_origin\"}\n          />\n        </div> */}\n        <div className=\"link-singup\">\n          <span>아직 계정이 없으신가요?</span>\n          <Link to=\"/signup\" style={{color:\"black\", textDecoration:\"none\"}}>\n          👉 회원가입하기\n          </Link>          \n        </div>    \n    </div>\n  )\n}\n\n","/Users/seunghunhan/Desktop/project/noti-client/src/components/SignupPage/SignupPage.js",[],"/Users/seunghunhan/Desktop/project/noti-client/src/components/DetailProductPage/DetailProductPage.js",["176","177","178","179","180"],"// link를 통해서 상세페이지에 갈 수 있다\n// 상품의 유니크아이디가 있다\n// 발매정보리스트에서 하나당 링크를 달아야함\n// 인프런 강의 randingpage에서 rendercard참고\n\n// 상세페이지 갖고 올때는 백엔드에게 그 상품에 맞는 아이디를 \n\nimport React, 